
if [ "$1" != "skip" ]; then
  ./bake_localnet.sh
  cd ./tezos

  
  docker ps | grep localnet_node_1 &> /dev/null
  if [ $? == 0 ]; then  # if process is running
    ./scripts/localnet.sh stop  # then stop the cluster
  fi

  ./scripts/localnet.sh start
  cd ..
fi

./localnet.sh client bootstrapped
./deploy_test_contract.sh
